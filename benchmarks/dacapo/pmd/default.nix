{ stdenv, fetchzip, ant, daCapoSrc, lib}:
{ 
  name = "pmd";
  mainclass = "net.sourceforge.pmd.PMD";
  jarfile = "pmd-4.2.5.jar";
  build = java: rec {
    version = "4.2.5";
    src = fetchzip {
      # Same file different mirror
      url = ''http://skylineservers.dl.sourceforge.net/project/pmd/pmd/4.2.5/pmd-src-4.2.5.zip'';
      md5 = "de56d016ee0e901a743a7d57cc4df991";
    };
    phases = [ "unpackPhase" "buildPhase" "installPhase" ];
    buildInputs = [ ant java.jdk ];
    buildPhase = ''
      ant jar
    '';
    inherit daCapoSrc;
    installPhase = ''
      mkdir -p $out/share/java
      mv lib/pmd-4.2.5.jar $_ 
      mv lib/asm-3.1.jar $_ 

      # Datafiles
      mkdir -p $out/data
      mv rulesets $out/data
      mv src $out/data
      cp -r $daCapoSrc/benchmarks/bms/pmd/data/pmd $out/data
    '';
  };
  libraries = java: with java.libs; [ jaxen java.libs.ant ];
  inputs = let 
    inputs = [
      { name = "xsmall";
        args = [ 
          (fixpath smallargs)
          "xml" 
          "$data/data/rulesets/basic.xml" 
          "-debug"
        ];
      }
      { name = "small";
        args = [ (fixpath smallargs) "xml" ] ++ baseargs;
      }
      { name = "default";
        args = [ (fixpath defaultargs) "xml" ] ++ baseargs;
      }
      { name = "large";
        args = [ (fixpath largeargs) "xml" ] ++ baseargs;
      }
    ];
    baseargs = [
        "$data/data/rulesets/basic.xml"
        "$data/data/rulesets/braces.xml"
        "$data/data/rulesets/codesize.xml"
        "$data/data/rulesets/controversial.xml"
        "$data/data/rulesets/coupling.xml"
        "$data/data/rulesets/design.xml"
        "$data/data/rulesets/favorites.xml"
        "$data/data/rulesets/finalizers.xml"
        "$data/data/rulesets/imports.xml"
        "$data/data/rulesets/javabeans.xml"
        "$data/data/rulesets/junit.xml"
        "$data/data/rulesets/naming.xml"
        "$data/data/rulesets/newrules.xml"
        "$data/data/rulesets/rulesets.properties"
        "$data/data/rulesets/scratchpad.xml"
        "$data/data/rulesets/strictexception.xml"
        "$data/data/rulesets/strings.xml"
        "$data/data/rulesets/unusedcode.xml"
    ];
